<DEFINE LFCN LFCN (L "AUX" Y) 
	#DECL ((L) <LIST [REST OBJECT]> (Y) ADV (LFCN) ACTIVATION)
	<MAPF <>
	      <FUNCTION (X) 
		      #DECL ((X) OBJECT)
