<DEFINE MIROUT ("AUX" DIR (MDIR ,MDIR) RM)
        #DECL ((DIR) <OR ATOM DIRECTION FIX> (MDIR) FIX (RM) <OR FALSE ROOM>)
        <COND (<==? <2 ,PRSVEC> <FIND-DIR "EXIT">> <SET DIR T>)
