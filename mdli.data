			     <FUNCTION ()
				  #DECL ((ITM) ANY)
				  <SET ITM <READ .C '<MAPSTOP>>>
				  <COND (<TYPE? .ITM LIST>
					 <ZEVAL .ITM>)>
				  .ITM>>>
		  <CLOSE .C>
		  "Done")
		 (<ILLEGAL "File not found.">)>)
	  (<ILLEGAL "Non-string file name?">)>>

