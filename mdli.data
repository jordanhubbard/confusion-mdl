	<POP	TP* B>
	<POP	TP* A>			; "GET STRING BACK"
	<SUB	P* [<(5) 5>]>		; "CLEAN UP P"
	<JRST	MPOPJ>			; "DONE"
OPLOST	<MOVE	A* <TYPE-WORD FALSE>>	; "RETURN FALSE"
	<MOVEI	B* 0>
