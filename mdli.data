"PARG -- print info for one argument of a verb"

<DEFINE PARG (VARG "AUX" (B <VBIT .VARG>) (W <VWORD .VARG>) (OUTCHAN .OUTCHAN))
	#DECL ((VARG) VARG (B W) FIX (OUTCHAN) CHANNEL)
	<COND (<AND <0? .B> <0? .W>>)
