	<ADD	C* (P)>			; "END OF DESTINATION"
	<BLT	D* (C)>			; "GET STRING"
	<MOVEI	O* <TYPE-CODE STRING>>
	<DPB	O* [<(*221503*) 1>]>	; "CLOBBER DOPE WORDS"
	<HRLI	B* *440700*>
	<MOVSI	A* <TYPE-CODE STRING>>
	<HRR	A* -1(P)>		; "FINISH STRING POINTER"
	<PUSH	TP* A>			; "PUSH STRING"
	<PUSH	TP* B>
