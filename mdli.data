"ZPLUS -- two args"

<DEFINE ZPLUS (A B)
	<COND (<ASSIGNED? B> <+ .A .B>)
	      (ELSE .A)>>

"ZMINUS -- two args"

<DEFINE ZMINUS (A B)
	<COND (<ASSIGNED? B> <- .A .B>)
	      (ELSE .A)>>

"ZTIMES -- two args"

<DEFINE ZTIMES (A B)
	<COND (<ASSIGNED? B> <* .A .B>)
	      (ELSE .A)>>

"ZDIVIDED -- two args"

<DEFINE ZDIVIDED (A B)
	<COND (<ASSIGNED? B> </ .A .B>)
	      (ELSE .A)>>

"ZLESS -- two args"

<DEFINE ZLESS (A B)
	<COND (<ASSIGNED? B> <L? .A .B>)
	      (ELSE .A)>>

"ZGREATER -- two args"

<DEFINE ZGREATER (A B)
	<COND (<ASSIGNED? B> <G? .A .B>)
	      (ELSE .A)>>

"ZEQUAL -- two args"

<DEFINE ZEQUAL (A B)
	<COND (<ASSIGNED? B> <==? .A .B>)
	      (ELSE .A)>>

"ZAND -- two args"
	   
<DEFINE ZAND (A B)
	<COND (<ASSIGNED? B> <AND .A .B>)
	      (ELSE .A)>>

"ZOR -- two args"

<DEFINE ZOR (A B)
	<COND (<ASSIGNED? B> <OR .A .B>)
	      (ELSE .A)>>

"ZIS -- fsubr"

<DEFINE ZIS (OBJ LIST)
	<ZPRED .OBJ .LIST>>

"ZISNT -- fsubr"

<DEFINE ZISNT (OBJ LIST)
	<NOT <ZPRED .OBJ .LIST>>>

"ZPRED -- general predicates"

<DEFINE ZPRED (OBJ EXPR "OPTIONAL" (TTYPE <>)
	      "AUX" (NTTYPE <>) (VAL <>) (NOT? <>) (BOOL? <>))
	#DECL ((OBJ) ANY (EXPR) <OR ATOM LIST>)
	<COND (<TYPE? .EXPR ATOM>
	       <COND (<NOT .TTYPE>
		      <ZEQUALS .OBJ <ZEVAL .EXPR>>)
		     (<=? .TTYPE in>
		      <ZIN .OBJ <ZEVAL .EXPR>>)
		     (ELSE <ERROR UNKNOWN-TTYPE .TTYPE>)>)
	      (ELSE
	       <MAPF <>
		     <FUNCTION (E)
			 #DECL ((E) <OR ATOM LIST>)
			 <COND (<AND <TYPE? .E ATOM> <MEMQ .E ,BUZZ>>)
			       (<==? .E or> <SET BOOL? <>>)
			       (<==? .E and> <SET BOOL? T>)
			       (<==? .E not> <SET NOT? <NOT .NOT?>>)
			       (<MEMQ .E ,TEST-TYPES> <SET NTTYPE .E>)
			       (ELSE
				<SET NVAL <ZPRED .OBJ .E <OR .NTTYPE .TTYPE>>>
				<SET NTTYPE <>>
				<COND (.NOT?
				       <SET NVAL <NOT .NVAL>>
				       <SET NOT? <>>)>
				<COND (.BOOL?
				       <SET VAL <AND .VAL .NVAL>>)
				      (ELSE
				       <SET VAL <OR .VAL .NVAL>>)>)>>
		     .EXPR>
	       .VAL)>>

